{
  "name": "hearthstone",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "lint": "eslint --ext .js,jsx .",
    "lint:fix": "yarn run lint --fix",
    "clear": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules && npm cache clean --force && rm -rf package-lock.json && rm -rf ios/build && rm -rf android/app/build && rm -rf ios/Podfile.lock && rm -rf ios/Pods",
    "pods": "cd ios && pod install && cd ..",
    "fix": "yarn fix-webview",
    "intel-pod": "cd ios && arch -x86_64 pod install && cd ..",
    "fix-webview": "cp module_modifications/react-native-webview/apple/RNCWebView.m node_modules/react-native-webview/apple/RNCWebView.m",
    "apk": "cd android && ./gradlew assembleRelease",
    "fresh": "yarn clear && yarn && yarn pods && npx jetifier",
    "release": "npx react-native run-android --variant=release",
    "iphone6": "npx react-native run-ios --simulator='iPhone 6'",
    "iphone7plus": "npx react-native run-ios --simulator='iPhone 7 Plus'",
    "iphoneXsMax": "npx react-native run-ios --simulator='iPhone Xs Max'",
    "prettify": "yarn run lint:fix; prettier --write '**/*.{ts,tsx}' && prettier --write '*.{ts,tsx}'"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-navigation/native": "5.8.10",
    "@react-navigation/stack": "5.12.8",
    "@unimodules/core": "~6.0.0",
    "apisauce": "2.0.0",
    "eslint-config-prettier": "^8.3.0",
    "formik": "^2.2.8",
    "i18n-js": "3.8.0",
    "i18next": "^20.3.0",
    "lodash": "^4.17.21",
    "lottie-ios": "3.2.3",
    "lottie-react-native": "^4.0.3",
    "lunr": "^2.3.9",
    "lunr-languages": "^1.4.0",
    "moment": "^2.29.1",
    "ramda": "0.27.1",
    "react": "17.0.1",
    "react-i18next": "^11.9.0",
    "react-native": "0.64.1",
    "react-native-gesture-handler": "~1.8.0",
    "react-native-gifted-chat": "^0.16.3",
    "react-native-localize": "^2.1.0",
    "react-native-reanimated": "^2.2.0",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "~2.15.2",
    "react-native-unimodules": "~0.12.0",
    "react-redux": "^7.2.4",
    "reactotron-react-native": "5.0.0",
    "redux": "^4.1.0",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "validator": "^13.6.0",
    "yup": "^0.32.9",
    "react-native-svg": "12.1.0",
    "react-native-render-html": "^5.1.0",
    "html-entities": "^2.3.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.12.1",
    "@babel/plugin-proposal-optional-catch-binding": "7.12.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/i18n-js": "3.0.3",
    "@types/jest": "26.0.19",
    "@types/ramda": "0.27.32",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "@types/react-redux": "^7.1.16",
    "@types/react-test-renderer": "16.9.4",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "^4.25.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "bufferutil": "4.0.2",
    "detox": "17.14.5",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-standard": "16.0.2",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-native": "3.10.0",
    "jest": "25.5.4",
    "jest-circus": "25.5.4",
    "jetifier": "1.6.6",
    "mocha": "8.2.1",
    "npm-run-all": "4.1.5",
    "patch-package": "6.2.2",
    "postinstall-prepare": "1.0.1",
    "prettier": "^2.3.0",
    "react-devtools-core": "4.10.1",
    "react-dom": "16.13.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-web": "~0.13.12",
    "react-powerplug": "1.0.0",
    "react-test-renderer": "17.0.1",
    "rimraf": "3.0.2",
    "solidarity": "2.3.1",
    "typescript": "~4.0.0",
    "utf-8-validate": "5.0.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
